#main.container
	.main_image
		.image_thumb
			ul 		
				- each element in elements
					li(class=element.alternate)
						a(href='rotator/' + element.imageUrl)
						.content
							h2 #{element.headline}
							p!= element.caption
		img(src='rotator/' + elements[0].imageUrl, alt="- banner1") 
		.desc
			a.collapse(href="#", class="collapse") Close Me! 
			.block
				h2 #{elements[0].headline}
				p!= elements[0].caption

